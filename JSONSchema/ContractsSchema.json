{
    "$schema": "http://json-schema.org/schema#",
    "title": "Match contracts",
    "description": "This is a schema that matches contracts.",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "oid": {
                "type": "string"
            },
            "beitrag": {
                "type": "number"
            },
            "gesellschaft": {
                "type": "string"
            },
            "hauptfälligkeit": {
                "type": "string",
                "pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2})T([0-9]{2}):([0-9]{2}):([0-9]{2})$"
            },
            "produkt": {
                "type": "string"
            },
            "vermittlungsdatum": {
                "type": "string",
                "pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2})T([0-9]{2}):([0-9]{2}):([0-9]{2})$"
            },
            "vertragsende": {
                "type": "string",
                "pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2})T([0-9]{2}):([0-9]{2}):([0-9]{2})$"
            },
            "vertragsnummer": {
                "type": "string"
            },
            "zahlungsweise": {
                "type": "string",
                "enum": [
                    "jährlich",
                    "jaehrlich",
                    "jahrlich",
                    "halbjährlich",
                    "halbjaehrlich",
                    "halbjahrlich",
                    "vierteljährlich",
                    "vierteljaehrlich",
                    "vierteljahrlich",
                    "monatlich",
                    "einmalig"
                ]
            }
        },
        "required": [
            "oid",
            "beitrag",
            "gesellschaft",
            "hauptfälligkeit",
            "produkt",
            "vermittlungsdatum",
            "vertragsende",
            "vertragsnummer",
            "zahlungsweise"
        ]
    }
}
