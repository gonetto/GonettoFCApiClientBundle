{
    "$schema": "http://json-schema.org/schema#",
    "title": "Match documents",
    "description": "This is a schema that matches documents.",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "oid": {
                "type": "string"
            },
            "vertragID": {
                "type": "string"
            },
            "beschreibung": {
                "type": "string"
            },
            "datum": {
                "type": "string",
                "pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2})T([0-9]{2}):([0-9]{2}):([0-9]{2})$"
            }
        },
        "required": [
            "oid",
            "vertragID",
            "beschreibung",
            "datum"
        ]
    }
}
