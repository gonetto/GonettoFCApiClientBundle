{
    "$schema": "http://json-schema.org/schema#",
    "title": "Match documents",
    "description": "This is a schema that matches documents.",
    "type": "object",
    "properties": {
        "erstelltAm": {
            "type": "string",
            "pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2})$"
        },
        "data": {
            "type": "string"
        },
        "extension": {
            "type": "string"
        }
    },
    "required": [
        "erstelltAm",
        "data",
        "extension"
    ]
}
